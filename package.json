  "keywords": [
    "react",
    "react-component",
    "react-photo-view",
    "photo"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
