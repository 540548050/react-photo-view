  "devDependencies": {
    "@svgr/rollup": "^4.1.0",
    "@types/jest": "^24.0.11",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.uniqueid": "^4.0.6",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "@types/styled-components": "^4.1.13",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
